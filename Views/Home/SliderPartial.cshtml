@model IEnumerable<KurumsalWeb.Models.Model.Slider>
    @{ 
        Layout = null;
    }

<div id="main-slider" class="flexslider">
    <ul class="slides">
        @foreach (var item in Model)
        {
            <li>
                <img src="@item.ResimURL" alt="@item.Baslik" width="1024" height="360"/>
            </li>
        }


    </ul>
</div>